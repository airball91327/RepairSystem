        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CancelDate)
        </td>
        @*<td>
            @Html.DisplayFor(modelItem => item.ShutDate)
        </td>*@
        <td>
            @Html.ActionLink("編輯", "Edit", new { id = item.TicketNo }) @*|*@
