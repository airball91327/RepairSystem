@model EDIS.Models.RepairModels.UnsignCounts
@{
    Layout = "~/Views/Shared/_PassedLayout.cshtml";
}

<h4>簽核列表</h4>
<ul class="nav nav-tabs">
    <li class="active"><a data-toggle="tab" href="#home">請修 <span class="badge" id="repairCount">@Model.RepairCount</span></a></li>
    <li><a data-toggle="tab" href="#menu2">保養 <span class="badge" id="keepCount">@Model.KeepCount</span></a></li>
</ul>
<div class="tab-content">
    <div id="home" class="tab-pane fade in active">
        <div>
            @await Component.InvokeAsync("RepIndex")
        </div>
    </div>
    <div id="menu2" class="tab-pane fade">
        <div>
            @await Component.InvokeAsync("KeepIndex")
        </div>
    </div>
</div>
